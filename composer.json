{
    "name": "costobrew/web-app",
    "description": "Costobrew Coffee Studio Web Application",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "kreait/firebase-php": "^6.0",
        "paragonie/sodium_compat": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1.0"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
